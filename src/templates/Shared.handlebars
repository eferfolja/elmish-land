module {{RootModule}}.Shared

open System
open System.Collections.Generic
open Elmish
open Feliz

type SharedModel = unit

type SharedMsg = | NoOp

let init (): SharedModel * CmdEx<'msg, SharedMsg> =
    (), CmdEx.none

let update (msg: SharedMsg) (model: SharedModel): SharedModel * CmdEx<'msg, SharedMsg> =
    match msg with
    | NoOp -> model, CmdEx.none
